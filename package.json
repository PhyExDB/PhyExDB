{
  "name": "nuxt-app",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "build:migrations": "tsc -p tsconfig.migrations.json",
    "build:all": "npm run build && npm run build:migrations",
    "dev": "npm run migrate && dotenv -e .env.development nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "vitest",
    "migrate": "npm run migrate:models && npm run migrate:seed",
    "migrate:models": "tsx scripts/migrate.ts",
    "migrate:seed": "tsx scripts/seed.ts",
    "migrate:revert": "tsx scripts/revert.ts",
    "migrate:status": "tsx scripts/migrationStatus.ts"
  },
  "dependencies": {
    "bcrypt": "^5.1.1",
    "nuxt": "^3.14.159",
    "pg": "^8.13.1",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.37.5",
    "umzug": "^3.8.2",
    "valibot": "^1.0.0-beta.8",
    "vue": "latest",
    "vue-router": "latest",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "@nuxt/eslint": "^0.7.1",
    "@nuxt/test-utils": "^3.14.4",
    "@playwright/test": "^1.49.0",
    "@types/bcrypt": "^5.0.2",
    "@types/pg": "^8.11.10",
    "@types/sequelize": "^4.28.20",
    "@types/uuid": "^10.0.0",
    "@vue/test-utils": "^2.4.6",
    "dotenv-cli": "^7.4.4",
    "eslint": "^9.15.0",
    "happy-dom": "^15.11.7",
    "playwright-core": "^1.49.0",
    "typescript": "^5.6.3",
    "vitest": "^2.1.6",
    "tsx": "^4.19.2"
  }
}
