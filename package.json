{
  "name": "nuxt-app",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "build:migrations": "tsc -p tsconfig.migrations.json",
    "build:all": "npm run build && npm run build:migrations",
    "dev": "npm run migrate && dotenv -e .env.development nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "npm run test:integration && npm run test:e2e",
    "test:integration": "vitest",
    "test:e2e": "playwright test",
    "migrate": "npm run migrate:models && npm run migrate:seed",
    "migrate:models": "tsx scripts/migrate.ts",
    "migrate:seed": "tsx scripts/seed.ts",
    "migrate:revert": "tsx scripts/revert.ts",
    "migrate:status": "tsx scripts/migrationStatus.ts"
  },
  "dependencies": {
    "@nuxt/icon": "^1.9.1",
    "@nuxtjs/color-mode": "^3.5.2",
    "@vueuse/core": "^12.0.0",
    "bcrypt": "^5.1.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-vue-next": "^0.468.0",
    "nuxt": "^3.14.159",
    "pg": "^8.13.1",
    "pg-hstore": "^2.3.4",
    "radix-vue": "^1.9.10",
    "sequelize": "^6.37.5",
    "shadcn-nuxt": "^0.11.3",
    "tailwind-merge": "^2.5.5",
    "tailwindcss-animate": "^1.0.7",
    "umzug": "^3.8.2",
    "valibot": "^1.0.0-beta.9",
    "vue": "latest",
    "vue-router": "latest",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "@iconify-json/heroicons": "^1.2.1",
    "@nuxt/eslint": "^0.7.1",
    "@nuxt/test-utils": "^3.14.4",
    "@nuxtjs/tailwindcss": "^6.12.2",
    "@playwright/test": "^1.49.0",
    "@types/bcrypt": "^5.0.2",
    "@types/node": "^22.10.1",
    "@types/pg": "^8.11.10",
    "@types/sequelize": "^4.28.20",
    "@types/uuid": "^10.0.0",
    "@vue/test-utils": "^2.4.6",
    "autoprefixer": "^10.4.20",
    "dotenv-cli": "^7.4.4",
    "eslint": "^9.16.0",
    "happy-dom": "^15.11.7",
    "playwright-core": "^1.49.0",
    "postcss": "^8.4.49",
    "tailwindcss": "^3.4.16",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2",
    "vitest": "^2.1.6"
  }
}
